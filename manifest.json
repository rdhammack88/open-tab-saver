{
    "name": "Tab Saver",
    "description": "Save opened tabs to JSON file",
    "version": "2.0",
    "permissions": ["activeTab", "tabs", "http://*/*", "https://*/*", "bookmarks"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Save opened tabs"
    },
    "manifest_version": 2
}